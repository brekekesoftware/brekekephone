+static AVAudioPlayer *audioPlayer = nil;
+
+- (void)playRingtoneWithURL:(NSURL *)url replay:(BOOL)replay {
+    if(!url) {
+        NSLog(@"[RNCallKeep] url does not exist");
+        return;
+    }
+    if (audioPlayer != nil && !replay) {
+        NSLog(@"[RNCallKeep] Ringtone is playing");
+        return;
+    }
+    NSError *error = nil;
+    
+    audioPlayer = [[AVAudioPlayer alloc] initWithContentsOfURL:url error:&error];
+    if (error || !audioPlayer) {
+        NSLog(@"[RNCallKeep] Failed to create AVAudioPlayer: %@", error.localizedDescription);
+        return;
+    }
+    audioPlayer.numberOfLoops = -1;
+    dispatch_after(dispatch_time(DISPATCH_TIME_NOW, (int64_t)(0.3 * NSEC_PER_SEC)), dispatch_get_main_queue(), ^{
+        [RNCallKeep configAudioSessionForAudioPlayer];
+        BOOL p = [audioPlayer prepareToPlay];
+        if(!p) {
+            NSLog(@"[RNCallKeep] Prepare failed!");
+            return;
+        }
+        dispatch_after(dispatch_time(DISPATCH_TIME_NOW, (int64_t)(0.3 * NSEC_PER_SEC)), dispatch_get_main_queue(), ^{
+            BOOL au = [audioPlayer play];
+            if(au) {
+                NSLog(@"[RNCallKeep] Play url %@ " , url.path);
+            }
+        });
+        
+    });
+}
+
+- (void)stopRingtone {
+    if (audioPlayer && callMap.count == 0) {
+        [audioPlayer stop];
+        audioPlayer = nil;
+        ringtoneUrl = nil;
+        NSLog(@"[RNCallKeep] Stopped ringtone");
+    }
+    
+}
+
++(void) configAudioSessionForAudioPlayer {
+    AVAudioSession *session = [AVAudioSession sharedInstance];
+    [session setCategory:AVAudioSessionCategoryPlayback
+             withOptions:AVAudioSessionCategoryOptionDuckOthers | AVAudioSessionCategoryOptionMixWithOthers
+                   error:nil];
+    
+    [session setActive:YES error:nil];
+}
+
+// listen interruption audio session
+- (void)handleAudioInterruption:(NSNotification *)notification {
+    NSDictionary *userInfo = notification.userInfo;
+    AVAudioSessionInterruptionType type = [userInfo[AVAudioSessionInterruptionTypeKey] integerValue];
+    if (type == AVAudioSessionInterruptionTypeBegan) {
+        if(audioPlayer != nil && audioPlayer.isPlaying) {
+            [audioPlayer pause];
+        }
+    }
+    else if (type == AVAudioSessionInterruptionTypeEnded) {
+        NSError *error = nil;
+        [[AVAudioSession sharedInstance] setActive:YES error:&error];
+        if (!error && ringtoneUrl && audioPlayer) {
+            [self playRingtoneWithURL:ringtoneUrl replay:YES];
+        }
+    }
+}
+
+- (void) removeListenAudioInterruption {
+    [[NSNotificationCenter defaultCenter] removeObserver:self
+                                                    name:AVAudioSessionInterruptionNotification
+                                                  object:nil];
+}
+
+- (void) listenAudioInterruption {
+    [[NSNotificationCenter defaultCenter] addObserver:self
+                                             selector:@selector(handleAudioInterruption:)
+                                                 name:AVAudioSessionInterruptionNotification
+                                               object:[AVAudioSession sharedInstance]];
+}
+
+// listen call event
+- (void)callObserver:(CXCallObserver *)callObserver callChanged:(CXCall *)call {
+    NSLog(@"[RNCallKeep] Call changed: %@, isOutgoing=%d, hasConnected=%d, hasEnded=%d",
+          call.UUID.UUIDString,
+          call.outgoing,
+          call.hasConnected,
+          call.hasEnded);
+    if(call.outgoing) {
+        [self handleOutgoingCall:call];
+    } else if ([RNCallKeep gsmCall:call.UUID]) {
+        [self handleGsmCall:call];
+    } else {
+        [self handleBrekekeCall:call];
+    }
+}
+
+- (void) handleOutgoingCall:(CXCall *) call {
+    NSLog(@"[RNCallKeep] is outgoing");
+    if(call.hasEnded || call.hasConnected) {
+        hasOutgoing = NO;
+        if (call.hasEnded && callMap.count > 0 && gsmCallMap.count == 0) {
+            [self playRingtoneWithURL:ringtoneUrl replay:NO];
+            NSLog(@"[RNCallKeep] play ringtone when outgoing call ended");
+        }
+        return;
+    }
+    hasOutgoing = YES;
+}
+
+- (void) handleGsmCall:(CXCall *) call {
+    NSUUID *uuid = call.UUID;
+    if(call.hasEnded) {
+        [gsmCallMap removeObjectForKey:uuid];
+        if (callMap.count > 0 && hasOutgoing == NO) {
+            [self playRingtoneWithURL:ringtoneUrl replay:NO];
+            NSLog(@"[RNCallKeep] play ringtone when gsm ended");
+        }
+        return;
+    }
+    gsmCallMap[uuid] = uuid;
+}
+
+- (void) handleBrekekeCall:(CXCall *) call {
+    NSUUID *uuid = call.UUID;
+    if(call.hasEnded || call.hasConnected) {
+        if (call.hasEnded) {
+            [callMap removeObjectForKey:uuid];
+        }
+        [self stopRingtone];
+        NSLog(@"[RNCallKeep] stop ringtone when call ended");
+        return;
+    }
+    if(callMap[uuid] && gsmCallMap.count == 0 && hasOutgoing == NO) {
+        NSLog(@"[RNCallKeep] play ringtone");
+        [self playRingtoneWithURL:ringtoneUrl replay:NO];
+    }
+}
+
+// validate
++ (BOOL) gsmCall:(NSUUID *)uuid {
+    if(callMap[uuid]) {
+        return NO;
+    }
+    return YES;
+}
+
++ (BOOL) localFile:(NSString *) r {
+    return [r hasPrefix:@"file://"];
+}

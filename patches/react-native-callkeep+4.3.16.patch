+// play https ringtone
+static AVAudioPlayer *audioPlayer = nil;
+
++ (void)playRingtoneWithURL:(NSURL *)url replay:(bool)replay {
+    
+    if(audioPlayer != nil && !replay) {
+        NSLog(@"[RNCallKeep] Audio is playing ");
+        return;
+    }
+    NSError *error = nil;
+    [RNCallKeep configAudioSessionForAudioPlayer];
+    audioPlayer = [[AVAudioPlayer alloc] initWithContentsOfURL:url error:&error];
+    if (error || !audioPlayer) {
+        NSLog(@"[RNCallKeep] Failed to create AVAudioPlayer: %@", error.localizedDescription);
+        return;
+    }
+    audioPlayer.numberOfLoops = -1;
+    dispatch_after(dispatch_time(DISPATCH_TIME_NOW, (int64_t)(0.5 * NSEC_PER_SEC)), dispatch_get_main_queue(), ^{
+        BOOL p = [audioPlayer prepareToPlay];
+        if(!p) {
+            NSLog(@"[RNCallKeep] Prepare failed!");
+            return;
+        }
+        
+        BOOL au = [audioPlayer play];
+        if(au) {
+            NSLog(@"[RNCallKeep] Play url %@ " , url.path);
+        }
+    });
+}
+
++ (void)stopRingtone {
+    if (audioPlayer) {
+        [audioPlayer stop];
+        audioPlayer = nil;
+        ringtoneUrl = nil;
+        NSLog(@"[RNCallKeep] Stopped ringtone");
+    }
+}
+
++(void) configAudioSessionForAudioPlayer {
+    AVAudioSession *session = [AVAudioSession sharedInstance];
+    [session setCategory:AVAudioSessionCategoryPlayback
+                       withOptions:AVAudioSessionCategoryOptionDuckOthers | AVAudioSessionCategoryOptionMixWithOthers
+                             error:nil];
+
+    [session setActive:YES error:nil];
+}
+
+// listen interruption audio session
+- (void)handleAudioInterruption:(NSNotification *)notification {
+    NSDictionary *userInfo = notification.userInfo;
+    AVAudioSessionInterruptionType type = [userInfo[AVAudioSessionInterruptionTypeKey] integerValue];
+    if (type == AVAudioSessionInterruptionTypeBegan) {
+        if(audioPlayer != nil && ringtoneUrl != nil) {
+            [RNCallKeep playRingtoneWithURL:ringtoneUrl replay:true];
+        }
+    }
+}
+
+- (void) removeListenAudioInterruption {
+    [[NSNotificationCenter defaultCenter] removeObserver:self
+                                                    name:AVAudioSessionInterruptionNotification
+                                                  object:nil];
+}
+
+- (void) listenAudioInterruption {
+    [[NSNotificationCenter defaultCenter] addObserver:self
+                                             selector:@selector(handleAudioInterruption:)
+                                                 name:AVAudioSessionInterruptionNotification
+                                               object:[AVAudioSession sharedInstance]];
+}
+
++(BOOL) localFile:(NSString *) r {
+    return [r hasPrefix:@"file://"];
+}
+
++ (BOOL)hasIncomingCallUI {
+    CXCallObserver *observer = [[CXCallObserver alloc] init];
+    for (CXCall *call in observer.calls) {
+        if (!call.hasConnected && !call.hasEnded) {
+            return YES;
+        }
+    }
+    return NO;
+}

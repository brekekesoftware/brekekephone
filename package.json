{
  "name": "brekekephone",
  "version": "2.7.17",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "craco start",
    "rn": "react-native start",
    "android": "react-native run-android",
    "shake": "adb shell input keyevent 82",
    "ios": "react-native run-ios",
    "bitcode": "[ -d node_modules/react-native-webrtc/ios/WebRTC.dSYM ] || bash node_modules/react-native-webrtc/tools/downloadBitcode.sh",
    "clean": "yarn && rm -rf ios/build/* && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && pod install && cd ../android && ./gradlew clean",
    "build": "del-cli -f node_modules/.cache/babel* && craco build",
    "intl": "del-cli -f .babelPluginIntl.json && cross-env EXTRACT_INTL=1 yarn build",
    "format": "eslint --ignore-path=.gitignore --ext=.js,.ts,.tsx --fix . && prettier --loglevel=error --ignore-path=.gitignore --write .",
    "check-all": "yarn check-eslint && yarn check-prettier && yarn check-typescript && yarn check-type-coverage",
    "check-eslint": "eslint --ignore-path=.gitignore --ext=.js,.ts,.tsx --max-warnings=0 .",
    "check-prettier": "prettier --ignore-path=.gitignore --list-different .",
    "check-typescript": "tsc",
    "check-type-coverage": "type-coverage --ignoreCatch --atLeast=100",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@mdi/js": "5.9.55",
    "@react-native-async-storage/async-storage": "1.14.1",
    "@react-native-community/push-notification-ios": "1.8.0",
    "brekekejs": "github:brekekesoftware/brekekejs#86d7442f88d05b60a16a205e9ccd323767bc81d8",
    "buffer": "6.0.3",
    "circular-json": "0.5.9",
    "es6-shim": "0.35.6",
    "es7-shim": "6.0.0",
    "eventemitter3": "4.0.7",
    "filesize": "6.1.0",
    "handlebars": "4.7.7",
    "helper-moment": "0.2.0",
    "json-stable-stringify": "1.0.1",
    "jssip": "3.2.15",
    "lodash": "4.17.21",
    "md5": "2.3.0",
    "mobx": "5.15.7",
    "mobx-react": "6.3.1",
    "moment": "2.29.1",
    "polished": "4.1.1",
    "prop-types": "15.7.2",
    "qs": "6.9.6",
    "react": "17.0.1",
    "react-art": "17.0.1",
    "react-device-detect": "1.17.0",
    "react-dom": "17.0.1",
    "react-native": "0.64.0",
    "react-native-background-timer": "2.4.1",
    "react-native-callkeep": "github:brekekesoftware/react-native-callkeep#9eff8243ba88df5c38474675eabbe75e0623458e",
    "react-native-document-picker": "4.3.0",
    "react-native-emoji-selector": "0.2.0",
    "react-native-exception-handler": "2.10.9",
    "react-native-fast-image": "8.3.4",
    "react-native-fcm": "github:brekekesoftware/react-native-fcm#afbddd1782afe9a6cf81c34f903e026b712d5579",
    "react-native-fs": "2.16.6",
    "react-native-hyperlink": "0.0.19",
    "react-native-image-picker": "2.3.4",
    "react-native-incall-manager": "3.3.0",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-share": "5.1.4",
    "react-native-splash-screen": "github:brekekesoftware/react-native-splash-screen#a78ee51c0f89652a2a223ff2bc4ec10928bf2c30",
    "react-native-svg": "12.1.0",
    "react-native-svg-web": "1.0.9",
    "react-native-uuid": "1.4.9",
    "react-native-voip-push-notification": "3.1.0",
    "react-native-web": "0.15.0",
    "react-native-web-linear-gradient": "1.1.1",
    "react-native-webrtc": "1.75.3",
    "url-parse": "1.5.1",
    "uuid": "8.3.2",
    "validatorjs": "3.22.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@craco/craco": "5.9.0",
    "@prettier/plugin-xml": "0.13.1",
    "@types/circular-json": "0.4.0",
    "@types/handlebars": "4.1.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jssip": "3.3.1",
    "@types/lodash": "4.14.168",
    "@types/md5": "2.3.0",
    "@types/qs": "6.9.6",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-native": "0.63.51",
    "@types/react-native-background-timer": "2.0.0",
    "@types/react-native-incall-manager": "3.2.0",
    "@types/react-native-keyboard-spacer": "0.4.1",
    "@types/react-native-share": "3.3.1",
    "@types/react-native-webrtc": "1.75.2",
    "@types/url-parse": "1.4.3",
    "@types/validatorjs": "3.15.0",
    "@typescript-eslint/parser": "4.17.0",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "7.0.3",
    "del-cli": "3.0.1",
    "eslint": "6.x",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fs-extra": "9.1.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "metro-react-native-babel-preset": "0.65.2",
    "prettier": "2.2.1",
    "react-scripts": "3.4.4",
    "type-coverage": "2.17.0",
    "typescript": "4.2.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,scss,json,md}": "prettier --write"
  },
  "cracoConfig": ".cracorc.js",
  "browserslist": "ie 10-11"
}
